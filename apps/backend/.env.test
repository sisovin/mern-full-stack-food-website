# Test environment variables

# MongoDB connection string for test database
MONGODB_URI=mongodb://localhost:27017/test_db

# JWT secret for test environment
JWT_SECRET=test_jwt_secret

# JWT expiration time for test environment
JWT_EXPIRES_IN=1h

# Redis URL for test environment
REDIS_URL=redis://localhost:6379

# API prefix for test environment
API_PREFIX=/api

# Rate limit window in milliseconds for test environment
RATE_LIMIT_WINDOW_MS=900000

# Rate limit max requests for test environment
RATE_LIMIT_MAX=100
